{
  "name": "ai-editor",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": "dist/main.js",
  "scripts": {
    "dev:next": "next dev --port 3030",
    "build:next": "next build",
    "start:next": "next start",
    "build:electron": "tsc --project electron/tsconfig.json",
    "watch:electron": "tsc --project electron/tsconfig.json --watch",
    "start:electron": "wait-on http://localhost:3030 && yarn build:electron && electron .",
    "dev": "concurrently -k \"yarn dev:next\" \"yarn start:electron\"",
    "build": "yarn build:next && yarn build:electron",
    "pack": "yarn build && electron-builder --dir",
    "dist": "yarn build && electron-builder"
  },
  "build": {
    "asar": false,
    "appId": "com.example.myapp",
    "productName": "MyApp",
    "files": [
      "dist/**/*",
      "out/**/*",
      "package.json",
      "!**/_assets",
      "!**/src"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "extraMetadata": {
      "main": "dist/main.js"
    },
    "mac": {
      "target": "dmg"
    },
    "win": {
      "target": "nsis"
    }
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@fal-ai/client": "^1.2.3",
    "@fal-ai/server-proxy": "^1.1.1",
    "@google-cloud/storage": "^7.15.2",
    "@radix-ui/react-aspect-ratio": "^1.1.2",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-scroll-area": "^1.2.4",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slider": "^1.2.3",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-switch": "^1.1.3",
    "@radix-ui/react-tabs": "^1.1.3",
    "@radix-ui/react-toggle": "^1.1.3",
    "@radix-ui/react-toggle-group": "^1.1.3",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@runware/sdk-js": "^1.1.37",
    "@tabler/icons-react": "^3.31.0",
    "@types/three": "^0.175.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "electron-is-dev": "^3.0.1",
    "electron-store": "^10.0.1",
    "install": "^0.13.0",
    "lucide-react": "^0.483.0",
    "next": "15.2.2",
    "next-themes": "^0.4.6",
    "posthog-js": "^1.234.1",
    "posthog-node": "^4.11.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "replicate": "^1.0.1",
    "sonner": "^2.0.3",
    "tailwind-merge": "^3.0.2",
    "three": "^0.175.0",
    "three-inspect": "^0.7.2",
    "tw-animate-css": "^1.2.4",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "electron": "^35.1.5",
    "electron-builder": "^26.0.12",
    "tailwindcss": "^4",
    "typescript": "^5",
    "wait-on": "^8.0.3"
  }
}
